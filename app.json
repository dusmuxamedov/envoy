{
  "expo": {
    "name": "envoy-app",
    "slug": "envoy-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "backgroundColor": "#000",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "/assets/fonts/Lora Font/Lora-Bold.ttf",
            "/assets/fonts/Lora Font/Lora-SemiBold.ttf",
            "/assets/fonts/Georgia/georgia.ttf",

            "/assets/fonts/Noto_Sans_Georgian/Georgiaa/NotoSansGeorgian_Condensed-Bold.ttf",
            "/assets/fonts/Noto_Sans_Georgian/Georgiaa/NotoSansGeorgian_Condensed-Regular.ttf",
            "/assets/fonts/Noto_Sans_Georgian/Georgiaa/NotoSansGeorgian_ExtraCondensed-Bold.ttf",

            "/assets/fonts/Roboto/static/Roboto-Regular.ttf",
            "/assets/fonts/Roboto/static/Roboto-Bold.ttf",
            "/assets/fonts/Roboto/static/Roboto-ExtraBold.ttf"
          ]
        }
      ]
    ],
    "scheme": "webcoder"
  }
}
